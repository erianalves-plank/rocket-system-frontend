{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "noEmit": true,
        "types": [
            "cypress"
        ]
    },
    "include": [
        "../node_modules/cypress",
        "**/*.ts",
        "/*.ts",
        "../cypress",
        "**/*.cy.tsx",
        "/*.cy.tsx",
        "**/*.cy.ts",
        "/*.cy.ts",
        "../cypress.config.ts",
    ]
}